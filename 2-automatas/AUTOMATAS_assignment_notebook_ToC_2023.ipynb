        "from automata.fa.nfa import NFA\n",
        "\n",
        "my_nfa = NFA(\n",
        "    states={'q0', 'q1', 'q2'},\n",
        "    input_symbols={'a', 'b'},\n",
        "    transitions={\n",
        "        'q0': {'a': {'q1'}},\n",
        "        # Use '' as the key name for empty string (lambda/epsilon) transitions\n",
        "        'q1': {'a': {'q1'}, '': {'q2'}},\n",
        "        'q2': {'b': {'q0'}}\n",
        "    },\n",
        "    initial_state='q0',\n",
